    is (any16 ; daAop=0x0 & RS & PAIR0 & PAIR1 & daDst0 & daDst1) {}
:"AddSub4x8" (daDst0, daDst1) = "BYTEOP16PM"(PAIR0, PAIR1) RS 
    is (any16 ; daAop=0x1 & RS & PAIR0 & PAIR1 & daDst0 & daDst1) {}
