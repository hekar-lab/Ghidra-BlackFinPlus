## Shift Immediate (Dsp32ShfImm)
##
## +---+---+---+---|---+---+---+---|---+---+---+---|---+---+---+---+
## | 1 | 1 | 0 | 0 |.m.| 1 | 1 | 0 | 1 |.dead..|.sopc..............|
## |.sop...|.hls...|.dst.......|.imm...................|.src.......|
## +---+---+---+---|---+---+---+---|---+---+---+---|---+---+---+---+

dsiNegImm4: imm is dsiImmS4 [imm = 0x10 - dsiImmS4;]{} 
dsiNegImm5: imm is dsiImmS5 [imm = 0x20 - dsiImmS5;]{} 

# AHSH4: "<<< 0" is dsiImm=0x0 {}
AHSH4: "<<<" dsiImmS4 is dsiImmP2=0x0 & dsiImmS4 {}
AHSH4: ">>>" dsiNegImm4 is dsiImmP2=0x3 & dsiNegImm4 {}

# ASH5: "<<< 0" is dsiImm=0x0 {}
ASH5: "<<<" dsiImmS5 is dsiImmP1=0x0 & dsiImmS5 {}
ASH5: ">>>" dsiNegImm5 is dsiImmP1=0x1 & dsiNegImm5 {}

# LHSH4: "<< 0" is dsiImm=0x0 {}
LHSH4: "<<" dsiImmS4 is dsiImmP2=0x0 & dsiImmS4 {}
LHSH4: ">>" dsiNegImm4 is dsiImmP2=0x3 & dsiNegImm4 {}

# LSH5: "<< 0" is dsiImm=0x0 {}
LSH5: "<<" dsiImmS5 is dsiImmP1=0x0 & dsiImmS5 {}
LSH5: ">>" dsiNegImm5 is dsiImmP1=0x1 & dsiNegImm5 {}

with : (opcode04=0xc & dsiOpcode=0xd) ... {
with : dsiSopc=0x00 ; dsiSop=0x0 {
:"AShift16" dsiDstL = dsiSrcL AHSH4 is any16 ; dsiHls=0x0 & dsiDstL & AHSH4 & dsiSrcL {}
:"AShift16" dsiDstL = dsiSrcH AHSH4 is any16 ; dsiHls=0x1 & dsiDstL & AHSH4 & dsiSrcH {}
:"AShift16" dsiDstH = dsiSrcL AHSH4 is any16 ; dsiHls=0x2 & dsiDstH & AHSH4 & dsiSrcL {}
:"AShift16" dsiDstH = dsiSrcH AHSH4 is any16 ; dsiHls=0x3 & dsiDstH & AHSH4 & dsiSrcH {}
}

with : dsiSopc=0x00 ; dsiSop=0x1 {
:"AShift16" dsiDstL = dsiSrcL AHSH4 "(S)" is any16 ; dsiHls=0x0 & dsiDstL & AHSH4 & dsiSrcL {}
:"AShift16" dsiDstL = dsiSrcH AHSH4 "(S)" is any16 ; dsiHls=0x1 & dsiDstL & AHSH4 & dsiSrcH {}
:"AShift16" dsiDstH = dsiSrcL AHSH4 "(S)" is any16 ; dsiHls=0x2 & dsiDstH & AHSH4 & dsiSrcL {}
:"AShift16" dsiDstH = dsiSrcH AHSH4 "(S)" is any16 ; dsiHls=0x3 & dsiDstH & AHSH4 & dsiSrcH {}
}

with : dsiSopc=0x00 ; dsiSop=0x2 {
:"LShift16" dsiDstL = dsiSrcL LHSH4 is any16 ; dsiHls=0x0 & dsiDstL & LHSH4 & dsiSrcL {}
:"LShift16" dsiDstL = dsiSrcH LHSH4 is any16 ; dsiHls=0x1 & dsiDstL & LHSH4 & dsiSrcH {}
:"LShift16" dsiDstH = dsiSrcL LHSH4 is any16 ; dsiHls=0x2 & dsiDstH & LHSH4 & dsiSrcL {}
:"LShift16" dsiDstH = dsiSrcH LHSH4 is any16 ; dsiHls=0x3 & dsiDstH & LHSH4 & dsiSrcH {}
}

with : dsiSopc=0x01 ; dsiHls=0x0 {
:"AShift16Vec" dsiDst = dsiSrc AHSH4 "(V)"   is any16 ; dsiSop=0x0 & dsiDst & AHSH4 & dsiSrc {}
:"AShift16Vec" dsiDst = dsiSrc AHSH4 "(V,S)" is any16 ; dsiSop=0x1 & dsiDst & AHSH4 & dsiSrc {}
:"LShift16Vec" dsiDst = dsiSrc LHSH4 "(V)"   is any16 ; dsiSop=0x2 & dsiDst & LHSH4 & dsiSrc {}
}

with : dsiSopc=0x02 ; dsiHls=0x0 {
:"AShift32"   dsiDst = dsiSrc ASH5              is any16 ; dsiSop=0x0 & dsiDst & ASH5 & dsiSrc {}
:"AShift32"   dsiDst = dsiSrc ASH5 "(S)"        is any16 ; dsiSop=0x1 & dsiDst & ASH5 & dsiSrc {}
:"LShift"     dsiDst = dsiSrc LSH5              is any16 ; dsiSop=0x2 & dsiDst & LSH5 & dsiSrc {}
:"ShiftRot32" dsiDst = "ROT" dsiSrc "BY" dsiImm is any16 ; dsiSop=0x3 & dsiDst & dsiImm & dsiSrc {}
}

with : dsiSopc=0x03 ... {
:"AShiftAcc"   "A0 = A0" ASH5          is any16 ; dsiSop=0x0 & dsiHls=0x0 & ASH5 {}
:"AShiftAcc"   "A1 = A1" ASH5          is any16 ; dsiSop=0x0 & dsiHls=0x1 & ASH5 {}
:"LShiftA"     "A0 = A0" LSH5          is any16 ; dsiSop=0x1 & dsiHls=0x0 & LSH5 {}
:"LShiftA"     "A1 = A1" LSH5          is any16 ; dsiSop=0x1 & dsiHls=0x1 & LSH5 {}
:"ShiftRotAcc" "A0 = ROT A0 BY" dsiImm is any16 ; dsiSop=0x2 & dsiHls=0x0 & dsiImm {}
:"ShiftRotAcc" "A1 = ROT A1 BY" dsiImm is any16 ; dsiSop=0x2 & dsiHls=0x1 & dsiImm {}
}
}
}

