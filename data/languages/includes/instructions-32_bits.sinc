:"Avg4x8Vec" daDst0 = "BYTEOP2P"(PAIR0, PAIR1) ("RNDH"RSC) is (daHl=0x1 ; daAop=0x0 & RSC & PAIR0 & PAIR1 & daDst0) {}
:"Avg4x8Vec" daDst0 = "BYTEOP2P"(PAIR0, PAIR1) ("TL"RSC)   is (daHl=0x0 ; daAop=0x1 & RSC & PAIR0 & PAIR1 & daDst0) {}
:"Avg4x8Vec" daDst0 = "BYTEOP2P"(PAIR0, PAIR1) ("TH"RSC)   is (daHl=0x1 ; daAop=0x1 & RSC & PAIR0 & PAIR1 & daDst0) {}
}

with : daAopc=0x17 ; daAop=0x0 & daX=0x0 {
